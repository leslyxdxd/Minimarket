create PROCEDURE [dbo].[usp_HabilitarProducto]
    @Id_Producto nchar(4)
AS
BEGIN
    -- Inhabilita un usuario cambiando su estado a 2
    UPDATE Tb_Productos
    SET Estado = 1,
        Fec_Registro = GETDATE()
    WHERE Id_Producto = @Id_Producto
END
