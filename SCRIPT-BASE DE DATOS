CREATE PROCEDURE [dbo].[usp_ActualizarUsuario]
    @Login_Usuario varchar(20),
    @Pass_Usuario varchar(20),
    @Niv_Usuario int,
    @Usu_Ult_Mod varchar(20)
AS
BEGIN
