    SET Pass_Usuario = @Pass_Usuario,
        Niv_Usuario = @Niv_Usuario,
        Fec_Registro = GETDATE(),
        Usu_Ult_Mod = @Usu_Ult_Mod
    WHERE Login_Usuario = @Login_Usuario
END
