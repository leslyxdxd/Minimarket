/****** Object:  View [dbo].[vw_Boletas]    Script Date: 19/07/2024 20:26:02 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[vw_Boletas] AS
SELECT
	  b.Id_Boleta,
	  b.Nro_boleta,
	 b.Nombres_Cliente,
    b.Apellidos_Cliente,
    b.Dni_Cliente,
    b.Fecha,
    dp.Id_DetalleBoleta,
    p.Id_Producto,
    p.Nom_Producto,
    p.Estado,
    p.Marca,
    p.Precio_Unitario,
    dp.Cantidad,
    dp.Igv,
    dp.subtotal,
	 dp.Total,
	 b.Fec_Registro,
    b.Usu_Registro,
    b.Fec_Ult_Mod,
    b.Usu_Ult_Mod
FROM 
    dbo.Tb_Detalle_Boleta dp
JOIN 
    dbo.Tb_Boleta b ON dp.Id_Boleta = b.Id_Boleta
JOIN 
    dbo.Tb_Productos p ON dp.Id_Producto = p.Id_Producto;
GO
