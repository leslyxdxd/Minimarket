create PROCEDURE [dbo].[sp_RestarStockTienda]
    @idproducto varchar(4),
    @cantidad INT
AS
BEGIN
    UPDATE Tb_Stock
    SET Stk_Tienda = Stk_Tienda - @cantidad
    WHERE Id_Producto = @idproducto;
END;
